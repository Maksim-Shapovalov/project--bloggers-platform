{

  "version": 2,

  "name": "project-for-incubator",

  "builds": [

    { "src": "src/main.ts", "use": "@vercel/node" }

  ],

  "routes": [
    {
      "src": "/",
      "status": 301,
      "headers": { "Location": "/api" }
    },
    {
      "src": "/api/(.*)",
      "dest": "src/main.ts"
    }
  ]

}